{
  "name": "webpack16-7",
  "version": "1.0.0",
  "main": "script.js",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:html": "globstar nu-html-checker *.html",
    "init-project": "npm install && npm-run-all init:*",
    "init:dirs": "mkdirp sass css vendor images js",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "build": "npm-run-all build:* test",
    "build:clean": "rimraf -rf css/*npm install npm-run-all --save-dev",
    "build:sass": "node-sass --output-style compact -o css sass",
    "build:autoprefixer": "autoprefixer-cli css/style.css",
    "build-dev:sass": "node-sass --output-style expanded --source-map true -o css sass",
    "watch": "npm-run-all build -p watch:*",
    "watch:sassprefixer": "onchange sass/*.scss -- npm-run-all build:sass -p build:autoprefixer",
    "watch:browsersync": "browser-sync start --server --files css/*.css *.html",
    "start": "babel script.js --watch --out-file script-compiled.js",
    "start-app": "babel components/Title.js --watch --out-file app-compiled.js",
    "start-gif": "babel components/gif.js --watch --out-file gif-compiled.js",
    "start-search": "babel components/search.js --watch --out-file search-compiled.js",
    "webpack:dev": "webpack-dev-server --hot --env development",
    "webpack": "webpack --env production"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.24.6",
    "css-loader": "^1.0.0",
    "globstar": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "node-sass": "^4.9.2",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.3",
    "onchange": "^4.1.0",
    "optimize-js-plugin": "0.0.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "description": "",
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "prop-types": "^15.6.2",
    "uuid": "^3.3.2"
  }
}
